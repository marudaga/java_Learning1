package ch04.unit5;

import java.util.Scanner;

public class ContinueEx02 {
	public static void main(String[] args) {
		//5개의 실수를 입력 받아 0보다 큰 수의 합 구하기
		Scanner sc= new Scanner(System.in);
		double input, s;
		s=0;
		System.out.print("실수 5개?");
		for(int i=0;i<5;i++) {
			input= sc.nextDouble();
			if(input<0.0) {
				s+=input;
			}
			s+=input;
		
		}
		System.out.println("결과:"+s);
		sc.close();
		
	}
}
