package ch04.unit3;

import java.util.Scanner;

//기본5번
public class ForQuiz03 {
//10개의 정수를 입력받아 입력받은 정수 중 가장 큰 값 출력
	public static void main(String[] args) {
		int a, b, big;
		Scanner sc = new Scanner(System.in);
		System.out.print("10개의 정수를 입력 하세요...");
		for (int i = 0; i < 10; i++) {
			a = sc.nextInt();
			b = sc.nextInt();
			if (a >= b) {
				big = a;
			} else {
				big = b;
			}
		}
		sc.close();
	}
}
