package ch04.unit3;

/*		
	A~Z 까지의 영문자를 한 줄에 5개씩 출력
*/
public class Quiz003_Alphabet5 {
	public static void main(String[] args) {
		char ch = 'A';
		int cnt;

		cnt = 0;
		while (ch <= 'Z') {
			System.out.print(ch + "\t");

			cnt++;
			if (cnt == 5) {
				System.out.println();
				cnt = 0;
			}

			ch++;
		}
	}
}
