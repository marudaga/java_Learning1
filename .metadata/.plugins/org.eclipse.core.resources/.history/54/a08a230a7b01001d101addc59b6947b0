package ch04.unit3;

/*		
	가로 출력 구구단
*/
public class Quiz010_Gugudan {
	public static void main(String[] args) {
		int dan, n;

		n = 0;
		while (n < 9) {
			n++;

			dan = 2;
			while (dan <= 9) {
				System.out.printf("%d*%d=%2d  ", dan, n, dan * n);
				dan++;
			}
			System.out.println();
		}
	}
}
