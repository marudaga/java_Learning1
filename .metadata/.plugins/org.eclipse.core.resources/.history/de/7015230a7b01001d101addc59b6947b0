package ch04.unit3;

/*		
	피보나치수열 1+1+2+3+5+8+13+21 합
*/
public class Quiz009_Fibonacci {
	public static void main(String[] args) {
		int a = 1, b = 1, c;
		int s = 2;

		int n = 2;
		while (n < 8) {
			c = a + b;
			s += c;
			a = b;
			b = c;
			n++;
		}
		System.out.println("결과 : " + s); // 54
	}
}
