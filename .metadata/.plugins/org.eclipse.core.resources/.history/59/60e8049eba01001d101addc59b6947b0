package ch04.unit5;

import java.util.Scanner;
// 잘못 입력되어 실행된 내용을 되돌릴 때
//continue에서 i-- 롤 잘못 입력한 값을 되돌릴 수 있다.
public class ContinueEx03 {
	public static void main(String[] args) {
		//
//		i:1, j:1
//		i:1, j:2
//		i:2, j:1
//		i:2, j:3
//		i:3, j:2
//		i:3, j:3
		jump:
		for(int i=1; i<=3; i++) {
			for(int j=1; j<=3; j++) {
				if(i+j==4) {
					continue jump;
				} 
				System.out.println("i:"+i+", j:"+j);
			}
		}
	}
}
